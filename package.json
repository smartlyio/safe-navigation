{
  "name": "@smartlyio/safe-navigation",
  "license": "MIT",
  "version": "4.0.1",
  "description": "typescript and null safe json object navigation",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "yarn jest test/",
    "render": "yarn ts-node render.ts",
    "build": "rm -rf ./dist/* && yarn lint && yarn test && yarn render && tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/safe-navigation.git"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "keywords": [
    "safe navigation",
    "typescript",
    "union type",
    "elvis operator",
    "optional chaining"
  ],
  "devDependencies": {
    "@types/jest": "26.0.3",
    "@types/lodash": "4.14.150",
    "@types/node": "11.15.16",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.17.1",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "26.1.0",
    "jsverify": "0.8.4",
    "prettier": "2.0.5",
    "ts-jest": "25.5.1",
    "ts-node": "8.10.2",
    "typescript": "3.6.3"
  },
  "jest": {
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "testRegex": "test/.*\\.spec.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
